<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<properties resource="jdbc.properties" />
	
	<!-- 设置别名 -->
	<typeAliases>
		<typeAlias alias="tbUser" type="com.tb.pojo.TbUser"/>
		<typeAlias alias="tbTw" type="com.tb.pojo.TbTw"/>
		<typeAlias alias="tbuserattention" type="com.tb.pojo.TbUserAttention"/>
		<typeAlias alias="tbComment" type="com.tb.pojo.TbComment"/>
		<typeAlias alias="tbmsg" type="com.tb.pojo.TbMsg"/>
	</typeAliases>
	
	<!-- 加载mapper映射 -->
	<mappers>
		<mapper resource="com/tb/mapper/UserMapper.xml"/>
	</mappers>
</configuration>